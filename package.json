{
  "name": "phox",
  "version": "0.1.0-alpha.24",
  "description": "Create a photo blog powered by Next.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "./script/clean",
    "dev": "tsc --watch",
    "prepublishOnly": "npm-run-all clean build",
    "lint:examples": "eslint --quiet './examples/simple/'",
    "lint:phox": "tslint --type-check --format stylish --project tsconfig.json",
    "test": "./script/test"
  },
  "author": "Emanuel Kluge (https://www.emanuel-kluge.de/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herschel666/phox.git"
  },
  "bugs": {
    "url": "https://github.com/herschel666/phox/issues"
  },
  "homepage": "https://github.com/herschel666/phox#readme",
  "dependencies": {
    "@types/deepmerge": "^1.3.2",
    "@types/delay": "2.0.0",
    "@types/express": "4.0.37",
    "@types/globby": "6.1.0",
    "@types/marked": "0.3.0",
    "@types/minimist": "1.2.0",
    "@types/mkdirp": "0.5.1",
    "@types/next": "2.4.2",
    "@types/node": "8.0.26",
    "@types/pretty-ms": "3.0.0",
    "@types/rimraf": "2.0.2",
    "@types/utf8": "2.1.6",
    "deepmerge": "^1.5.1",
    "delay": "2.0.0",
    "exif": "0.6.0",
    "express": "4.15.4",
    "front-matter": "2.2.0",
    "globby": "6.1.0",
    "imagemin": "5.3.1",
    "imagemin-jpegtran": "5.0.2",
    "imagemin-pngquant": "5.0.1",
    "marked": "0.3.6",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "next": "3.2.2",
    "node-iptc": "1.0.4",
    "pretty-ms": "3.0.0",
    "probe-image-size": "3.1.0",
    "rimraf": "2.6.1",
    "utf8": "2.1.2"
  },
  "bin": {
    "phox": "./bin/phox.js"
  },
  "devDependencies": {
    "@types/jest": "20.0.8",
    "@types/request": "2.0.3",
    "@types/request-promise": "4.1.37",
    "babel-eslint": "8.0.0",
    "eslint": "4.6.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.3.0",
    "jest": "21.0.2",
    "npm-run-all": "4.1.1",
    "prettier": "1.6.1",
    "request": "2.81.0",
    "request-promise": "4.2.1",
    "ts-jest": "21.0.0",
    "tslint": "5.7.0",
    "tslint-microsoft-contrib": "5.0.1",
    "typescript": "2.5.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.tsx?$",
    "testPathIgnorePatterns": [
      "node_modules",
      "__snapshots__",
      "test/.*/.*\\.js$"
    ],
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json"]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": false
  },
  "typings": "./typings.d.ts"
}
